{
    "name": "bear/package",
    "description": "BEAR.Sunday framework package",
    "keywords": [
        "BEAR.Sunday",
        "DI",
        "AOP",
        "REST",
        "framework",
        "hypermedia",
        "API"
    ],
    "homepage": "https://github.com/koriym/BEAR.Package",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "BEAR.Package Contributors",
            "homepage": "https://github.com/koriym/BEAR.Package/graphs/contributors"
        }
    ],
    "provide": {
        "bear/framework": "1.0.0"
    },
    "require": {
        "php": ">=5.5.0",
        "bear/sunday": "~1.0@dev",
        "koriym/psr4list": "~1.0@dev",
        "classpreloader/classpreloader": "~1.1@dev"
    },
    "require-dev": {
        "facebook/xhprof": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "BEAR\\Package\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BEAR\\Package\\": "tests/",
            "FakeVendor\\HelloWorld\\": "tests/Fake/FakeVendor/HelloWorld/src"
        }
    },
    "bin": [
        "bin/bear.compile",
        "bin/bear.create-resource",
        "bin/bear.server",
        "bin/bear.env"
    ],
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
